{
  "manifest_version": 3,
  "name": "Binaural Beats Generator",
  "version": "1.0",
  "description": "A Chrome extension to generate binaural beats.",
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  }
}
